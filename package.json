{
  "name": "lyfenode",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "if-env NODE_ENV=production && npm run server || npm run dev",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.6",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.4",
    "nodemon": "^1.18.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "plaid": "^2.9.0",
    "react": "^16.5.2",
    "react-chartjs-2": "^2.7.4",
    "react-chartkick": "^0.3.0",
    "react-dom": "^16.6.0",
    "react-plaid-link": "^1.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "validator": "^10.8.0"
  },
  "repository": "git@github.com:gyunker/lyfenode.git",
  "author": "NHolly <nickrhollenbeck@gmail.com>",
  "license": "MIT"
}
